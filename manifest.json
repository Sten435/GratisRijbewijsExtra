{
	"name": "Gratis Rijbewijs Online - Met Antwoorden",
	"version": "1.0",
	"manifest_version": 3,
	"content_scripts": [
		{
			"matches": ["*://*.gratisrijbewijsonline.be/*"],
			"run_at": "document_end",
			"js": ["inject.js", "alert.js"]
		}
	],
	"action": {
		"default_popup": "index.html"
	},
	"icons": {
		"16": "/images/logo.png",
		"32": "/images/logo.png",
		"48": "/images/logo.png"
	},
	"permissions": ["storage"],
	"host_permissions": ["<all_urls>"]
}
