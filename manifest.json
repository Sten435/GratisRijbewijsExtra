{
	"name": "Gratis Rijbewijs Online Examen Antwoorden",
	"version": "1.0",
	"manifest_version": 3,
	"content_scripts": [
		{
			"matches": ["*://*.gratisrijbewijsonline.be/*"],
			"run_at": "document_end",
			"js": ["inject.js", "alert.js"]
		}
	],
	"action": {
		"default_popup": "index.html"
	},
	"icons": {
		"16": "/images/icon16.png",
		"32": "/images/icon38.png",
		"48": "/images/icon48.png"
	},
	"permissions": ["storage", "tabs"],
	"host_permissions": ["<all_urls>"]
}
